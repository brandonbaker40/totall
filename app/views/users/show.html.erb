<% provide(:title, current_user.name) %>
<div class="row">
  <aside class="col-md-4">
    <section class="user_info">
      <h1>
        <%= gravatar_for current_user %>
        <%= current_user.name %>
      </h1>
    </section>
  </aside>
</div>
<h3>My Totall</h3>
<p><%= link_to number_to_currency(@totall), user_accounts_path(current_user_id) %></p>
